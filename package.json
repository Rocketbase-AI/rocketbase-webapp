{
  "name": "rocketbase-webapp",
  "version": "2.0.0",
  "description": "RocketBase web application",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc && tsc --project tsconfig.server.json",
    "start": "node .next/server",
    "serve": "npm run build && NODE_ENV=production node .next/server",
    "deploy": "gcloud app deploy --project=rockethub"
